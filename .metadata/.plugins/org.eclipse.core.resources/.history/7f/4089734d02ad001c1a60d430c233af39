package com.simple.basic.command;

public class BuilderVO {
	//빌더 패턴(디자인패턴)
	//1. 멤버변수
	private String name;
	private int age;
	
	
	//3. 외부에서 객체 생성을 요구할 때
	public static Builder builder() {
		return new Builder();
	}
	
	
	//2. 내부클래스
	public static class Builder {
		
		private String name;
		private int age;
		//생성자 제한
		private Builder() {}
		
	}
}
